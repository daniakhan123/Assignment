// the sum of given triplets makes the number x;


#include<stdio.h>
int main(){
	
	int arr[7]={1,2,3,4,5,6,7};
	int x = 12,totalpairs;
	
	for(int i=1;i<=7;i++){
		
		for(int j= i+1;j<=7;j++){
			for (int k=j+1;k<=7;k++)
			if(i+j+k==x){
				totalpairs++;
			printf("(%d,%d,%d)",j,i,k);
			}
		}
			
	}
	printf("\n %d",totalpairs);
			return 0;
	
}
